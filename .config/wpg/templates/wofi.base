#inner-box,
#outer-box,
#input,
#text {{
	margin: 5px;
	background-color: {color0};
}}

#window {{
	border: 4px solid {active};
	border-radius: 5px;
	color: {color15};
}}

#input {{
	border: none;
	border-bottom: 2px solid {active};
}}

#text {{
	color: {color15};
	font-size: 16px;
}}

#entry:selected,
#img:selected,
#text:selected {{
	background-color: {inactive};
}}
